<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyu.fire.mapper.UserMapper">

    <select id="selectByName" resultType="com.jyu.fire.pojo.User">
        select * from fire.user where username=#{username}
    </select>
</mapper>